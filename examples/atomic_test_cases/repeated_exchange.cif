<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<choreography xmlns="http://convecs.inria.fr">
    <choreoID>RepeatedExchangeChoreography</choreoID>
    <participants>
        <peer>
            <peerID>peer1</peerID>
        </peer>
        <peer>
            <peerID>peer2</peerID>
        </peer>
    </participants>
    <alphabet>
        <message>
            <msgID>mid</msgID>
            <sender>peer1</sender>
            <receiver>peer2</receiver>
            <messageContent>message</messageContent>
        </message>
        <message>
            <msgID>mid</msgID>
            <sender>peer2</sender>
            <receiver>peer1</receiver>
            <messageContent>message</messageContent>
        </message>
    </alphabet>
    <stateMachine>
        <initial>
            <stateID>StartEvent_1</stateID>
            <successors>ct1</successors>
        </initial>
        <interaction>
            <stateID>ct3</stateID>
            <successors>EndEvent_1</successors>
            <msgID>mid</msgID>
        </interaction>
        <interaction>
            <stateID>ct2</stateID>
            <successors>ct3</successors>
            <msgID>mid</msgID>
        </interaction>
        <interaction>
            <stateID>ct1</stateID>
            <successors>ct2</successors>
        </interaction>
        <final>
            <stateID>EndEvent_1</stateID>
        </final>
    </stateMachine>
</choreography>
